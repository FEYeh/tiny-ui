{
  "name": "core",
  "version": "0.0.1",
  "license": "MIT",
  "main": "component/index.ts",
  "devDependencies": {
    "@types/classnames": "^2.2.7",
    "@types/enzyme": "^3.9.0",
    "@types/jest": "^24.0.11",
    "@types/node": "^11.10.5",
    "@types/prop-types": "^15.7.0",
    "@types/react": "^16.8.7",
    "@types/react-dom": "^16.8.2",
    "@types/react-transition-group": "^2.0.16",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.11.2",
    "gulp": "^4.0.0",
    "gulp-clean-css": "^4.0.0",
    "jest": "^24.5.0",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "ts-jest": "^24.0.0",
    "ts-node": "^8.0.3",
    "tslint": "^5.13.1",
    "typescript": "^3.3.3333"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "react-icons": "^3.6.0-beta2",
    "react-transition-group": "^2.6.1"
  },
  "scripts": {
    "build": "rm -rf ../dist && tsc && gulp build",
    "test": "jest"
  },
  "jest": {
    "verbose": true,
    "setupFiles": [
      "<rootDir>/config/test-setup.js"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "testMatch": [
      "<rootDir>/component/**/__test__/*.{js,jsx,ts,tsx}"
    ],
    "transform": {
      "^.+\\.tsx?$": "<rootDir>/config/jest/typescriptTransform.js",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    }
  }
}
